{
    "name": "msami/tic-tac-toe",
    "description": "Tic-Tac-Toe php-cli game",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Mina Nabil Sami",
            "email": "mina.nsami@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.5",
        "phpstan/phpstan-doctrine": "^0.10.0",
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-0": {
            "TicTacToe": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "unit\\TicTacToe\\": "./tests/unit"
        }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    }
}
